#!/usr/bin/env bash
# Project Uriel manual activation script
# Run: source activate

# Load environment variables
if [[ -f .env.local ]]; then
    set -a  # automatically export all variables
    source .env.local
    set +a
fi

# Load project commands
if [[ -f scripts/commands.sh ]]; then
    source scripts/commands.sh
    echo "ЁЯЪА Project Uriel commands loaded! Type 'commands' for help."
else
    echo "тЪая╕П  Command scripts not found. Some features may not be available."
fi
